            android:name=".camera"
            android:exported="false" />
        <activity
            android:name=".posting"
            android:exported="false" />
        <activity
            android:name=".my_profile"
            android:exported="false" />
        <activity
            android:name=".you_page"
            android:exported="false" />
        <activity
            android:name=".following_page"
            android:exported="false" />
        <activity
            android:name=".my_story_view"
            android:exported="false" />
        <activity
            android:name=".story_post"
            android:exported="false" />
        <activity
            android:name=".story_view"
            android:exported="false" />
        <activity
            android:name=".edit_profile"
            android:exported="false" />
        <activity
            android:name=".story"
            android:exported="false" />
        <activity
            android:name=".call_page"
            android:exported="false" />
        <activity
            android:name=".notification_following"
            android:exported="false" />
        <activity
            android:name=".ChatActivity"
            android:exported="false" />
        <activity
            android:name=".dms"
            android:exported="false" />
        <activity
            android:name=".specific_search"
            android:exported="false" />
        <activity
            android:name=".search_feed"
            android:exported="false" />
        <activity
            android:name=".home_page"
            android:exported="false" />
        <activity
            android:name=".switch_account"
            android:exported="false" />
        <activity
            android:name=".signup_page"
            android:exported="false" />
        <activity
            android:name=".login_sign"
            android:exported="false" />
        <activity
            android:name=".login_page_2"
            android:exported="false"
            android:theme="@style/Theme.I230535" />
        <activity
            android:name=".login_page"
            android:exported="false"
            android:theme="@style/Theme.I230535" />
        <activity
            android:name=".sign_up_page"
            android:exported="false" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_notification" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/smd_theme" />
